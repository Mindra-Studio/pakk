{
  "name": "pakk",
  "version": "1.0.0",
  "description": "Smart compression with pre-trained dictionaries - 15-35% smaller bundles",
  "author": "Mindra Studio",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "compression",
    "zstd",
    "zstandard",
    "dictionary",
    "bundle",
    "vite",
    "nextjs",
    "webpack",
    "rollup",
    "esbuild",
    "parcel",
    "brotli",
    "gzip",
    "react",
    "vue",
    "typescript",
    "python",
    "rust",
    "go",
    "java",
    "cpp",
    "performance",
    "optimization",
    "build"
  ],
  "homepage": "https://github.com/mindra-studio/pakk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mindra-studio/pakk.git"
  },
  "bugs": {
    "url": "https://github.com/mindra-studio/pakk/issues"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pakk": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/plugins/vite.d.ts",
        "default": "./dist/plugins/vite.js"
      },
      "require": {
        "types": "./dist/plugins/vite.d.cts",
        "default": "./dist/plugins/vite.cjs"
      }
    },
    "./next": {
      "import": {
        "types": "./dist/plugins/next.d.ts",
        "default": "./dist/plugins/next.js"
      },
      "require": {
        "types": "./dist/plugins/next.d.cts",
        "default": "./dist/plugins/next.cjs"
      }
    },
    "./webpack": {
      "import": {
        "types": "./dist/plugins/webpack.d.ts",
        "default": "./dist/plugins/webpack.js"
      },
      "require": {
        "types": "./dist/plugins/webpack.d.cts",
        "default": "./dist/plugins/webpack.cjs"
      }
    },
    "./rollup": {
      "import": {
        "types": "./dist/plugins/rollup.d.ts",
        "default": "./dist/plugins/rollup.js"
      },
      "require": {
        "types": "./dist/plugins/rollup.d.cts",
        "default": "./dist/plugins/rollup.cjs"
      }
    },
    "./esbuild": {
      "import": {
        "types": "./dist/plugins/esbuild.d.ts",
        "default": "./dist/plugins/esbuild.js"
      },
      "require": {
        "types": "./dist/plugins/esbuild.d.cts",
        "default": "./dist/plugins/esbuild.cjs"
      }
    },
    "./parcel": {
      "import": {
        "types": "./dist/plugins/parcel.d.ts",
        "default": "./dist/plugins/parcel.js"
      },
      "require": {
        "types": "./dist/plugins/parcel.d.cts",
        "default": "./dist/plugins/parcel.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build && npm run test",
    "cli": "tsx src/cli/index.ts"
  },
  "dependencies": {
    "fflate": "^0.8.2",
    "picocolors": "^1.1.1",
    "tar": "^7.5.7",
    "undici": "^7.19.2"
  },
  "optionalDependencies": {
    "zstd-napi": "^0.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "@types/tar": "^6.1.13",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@parcel/core": ">=2.0.0",
    "esbuild": ">=0.17.0",
    "next": ">=13.0.0",
    "rollup": ">=3.0.0",
    "vite": ">=5.0.0",
    "webpack": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "@parcel/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
